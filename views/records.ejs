<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%=heading%>
    </title>
</head>

<body>
    <h1>
        <%=heading%>
    </h1>
    <ul>
        <%records.forEach(function(record) { %>
            <li>
                <a style="color: rgb(0, 0, 0);" href="/<%=heading%>/details/<%=record.name%>">
                    <%=record.name%>
                </a>
                <a style="color: rgb(255, 0, 0);" href="/<%=heading%>/delete/<%=record.name%>">
                    delete
                </a>
            </li>
            <%});%>
    </ul>
</body>

</html>